    "def fab(n):\n",
    "    if n <= 2: return(1)\n",
    "    else:\n",
    "        return(fab(n-1)+fab(n-2))\n",
    "n=20\n",
    "for i in range(1,n+1):\n",
    "    print(format(fab(i),\">5\"),end=' ')\n",
    "    if(i % 10==0):print(\"\\n\")\n"
