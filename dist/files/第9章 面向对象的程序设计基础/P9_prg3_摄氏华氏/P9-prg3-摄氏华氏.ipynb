    "class TemperatureCelsius:\n",
    "    def __init__(self, d):\n",
    "        self.degree = d\n",
    "    def ToFahrenheit(self):\n",
    "        return (self.degree * 9 / 5) + 32\n",
    "d = float(input(\"请输入摄氏温度： \"))\n",
    "celsius = TemperatureCelsius(d)\n",
    "print(celsius.ToFahrenheit())\n"
   ]
