   "source": [
    "def fact(n): #声明函数，返回值\n",
    "    if n==0:\n",
    "        f=1\n",
    "    else:\n",
    "        result=1\n",
    "        for i in range(1,n+1):\n",
    "            result*=i\n",
    "        f=result\n",
    "    return(f)\n",
    "n=int(input(\"请输入整数n（n>=0）：\"))\n",
    "print(fact(n))      #调用函数\n",
    "\n",
    "\n"
   ]
