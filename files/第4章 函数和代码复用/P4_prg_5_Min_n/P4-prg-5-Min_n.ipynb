   "source": [
    "def min_n(a, b, *c):\n",
    "    min_value = a\n",
    "    if min_value > b:\n",
    "        min_value = b\n",
    "    for n in c:\n",
    "        if min_value > n:\n",
    "            min_value = n\n",
    "    return min_value\n",
    "#测试代码\n",
    "print(\"最小值为\",min_n(8, 2))\n",
    "print(\"最小值为\",min_n(16, 1, 7, 4, 15))\n"
   ]
